		<parameter id="name" value="Owl2RlOptimized" />
		<parameter id="description" value="Repository config for GraphDB's built-in owl2-rl-optimized rule-set" />
		<parameter id="version" value="1.0" />
		<parameter id="priority" value="5" />
	</extension>

</plugin>
